		<div class="panel-body">
		<div class="row">
			<div class="container">
		<div class="col-md-12">	
			<div class="media">
				<div class="media-left">
					<a href="javascript:void(0)">
						<img class="media-object" src="<%= user.photos %>" alt="profile picture" height=80 >
					</a> 
				</div>
				<div class="media-body">
					<h4 class="media-heading singleuser"><strong><%= user.displayName %></strong></h4>
					<h5 class="media-heading"><strong><span class="badge"><%= user.score %>&nbsp;COINS</span></strong></h5>
					<h6 class="media-heading"><strong><span class="badge"><%= user.pairs %>&nbsp;PAIRS</span></strong></h6>
					<a href="javascript:void(0)" data-toggle="modal" data-target=".view-pairssummary-model">VIEW PAIRS</a>
					<div class="modal fade view-pairssummary-model" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
					  <div class="modal-dialog modal-lg" role="document">
						<div class="modal-content">
							<div class="panel panel-default">
								<div class="panel-heading">
									<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="glyphicon glyphicon-remove-sign"></span></button>
									<div class="btn-group" role="group" aria-label="Large button group"> 
										<button type="button" id="view_my_pairs" class="btn btn-md btn-sm btn-xs btn-success">VIEW MY PAIRS</button> 
										<button type="button" class="btn btn-md btn-sm btn-xs btn-info">VIEW TOP PLAYERS</button> 
										<button type="button" class="btn btn-md btn-sm btn-xs btn-default">VIEW TOP PAIRS</button> 
									</div>
								</div>
								<div class="row" id="showmypairs" style="padding:5px;">
								</div>
							</div>
						</div>
					  </div>
					</div>					
				</div><!-- /.row -->
				<div class="media-right">
							<button class="btn btn-info hidebutton" id="quickplay" type="button" style="margin-bottom:5px;width:180px">QUICK PLAY</button>
							<button id="createRoom" class="btn btn-success hidebutton" type="button" data-toggle="modal" data-target=".create-room-model" style="margin-bottom:5px;width:180px">CREATE ROOM</button>
							<form action="/battle" method="get">
								<input type="hidden" id="pairmania_id" name="pairmania_id" value="<%= user.pairmania_id %>"/>
								<input class="btn btn-primary btn-block" type="submit" value="PLAY BATTLE">
							</form>
							<form action="/singleplay" method="get">
								<input type="hidden" id="pairmania_id" name="pairmania_id" value="<%= user.pairmania_id %>"/>
								<input class="btn btn-primary btn-block" type="submit" value="SINGLE PLAYER">
							</form>
							<div class="modal fade create-room-model" tabindex="-1" role="dialog">
							  <div class="modal-dialog" role="document">
								<div class="modal-content">
								  <div class="modal-header">
									<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="glyphicon glyphicon-remove-sign"></span></button>
									<h4 class="modal-title">CREATE YOUR ROOM</h4>
								  </div>
								  <div class="modal-body">
									<p>
										<div class="input-group">
										  <span class="input-group-addon" id="basic-addon1">ROOM NAME</span>
										  <input type="text" id="roomname" class="form-control" placeholder="ROOM NAME" aria-describedby="ROOM NAME">
										</div>
									</p>
									<p>
										<div class="input-group">
										    <span class="input-group-addon" id="basic-addon1">PLAYER LIMIT</span>
											<select id="roomlimit" class="form-control">
											  <option value="">PLAYER LIMIT</option>
											  <option value="3">3</option>
											  <option value="4">4</option>
											  <option value="5">5</option>
											  <option value="6">6</option>
											  <option value="7">7</option>
											  <option value="8">8</option>
											</select>
										</div>
									</p>
								  </div>
								  <div class="modal-footer">
									<button type="button" class="btn btn-default" data-dismiss="modal">CLOSE</button>
									<button type="button" id="roombutton"  class="btn btn-primary">CREATE</button>
								  </div>
								</div><!-- /.modal-content -->
							  </div><!-- /.modal-dialog -->
							</div>
						</div><!-- /.col-lg-6 -->
					</div>
				</div>	
			</div>
			</div>
			<br>
			<div class="row">
				<div class="container">
				<div class="col-md-6 col-sm-6 col-xs-6">
					
					<div class="panel panel-default">
					<div class="panel-heading">
					<h3 class="panel-title">JOIN ROOMS</h3>
					</div>
					<div class="panel-body">
					
					<ul class="list-group" id="rooms">
					</ul>
					</div>
					</div>
				</div>
				<div class="col-md-6 col-sm-6 col-xs-6">
					<div class="panel panel-default">
					<div class="panel-heading">
					<h3 class="panel-title">INVITATIONS</h3>
					</div>
					<div class="panel-body">
					
					<ul class="list-group" id="invitations">
					</ul>
					</div>
					</div>
				</div>
			</div>
			</div>
		</div>
	</div>
	<script>
	$(document).ready(function(){
		
		ion.sound({
            sounds: [
                {name: "clock"},
                {name: "Retro-Frantic_V001_Looping"},
				{name: "switch"},
				{name: "happykids123"},
				{name: "Puzzle-Dreams-3"},
				{name: "lost"}
            ],
            path: "/",
            preload: true,
            volume: 1.0
        });
		
		socket.on('connect', function(){
		   // ion.sound.play("Puzzle-Dreams-3");
			playername=$('.singleuser').html();
			playerimg = $('#profile').attr('src');
			pairmania_userid = $('#pairmania_userid').attr('value');
			socket.emit('adduser',{
				Playerusername : playername, 
				Playerimg : playerimg, 
				pairmania_id : pairmania_userid,
				PlayerSocketId : socket.id
			});
		});
	
	});
		
</script>
<script src="/js/client.js"></script>
<% } %>
</body>
</html>